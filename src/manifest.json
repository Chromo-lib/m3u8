{
  "manifest_version": 3,
  "version": "1.0.8",
  "name": "HLS Streaming Player",
  "description": "MP4, WebM and HLS Streaming Player with free TV channels",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "34.png"
  },
  "icons": {
    "34": "34.png",
    "128": "128.png",
    "512": "512.png"
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}